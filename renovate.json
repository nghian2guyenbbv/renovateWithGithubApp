{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "baseBranches": ["develop"],
  "branchPrefix": "renovate/",
  "dependencyDashboardTitle":"Renovate Dashboard",
  "commitMessagePrefix": "Renovate update",
  "prHourlyLimit": 0,
  "branchConcurrentLimit": 0,
  "prConcurrentLimit": 0,
 "packageRules": [
 {
 "matchPackageNames": [
 "cimg/node",
 "node"
 ],
 "matchUpdateTypes": [
 "major",
 "minor"
 ],
 "enabled": false
 }
 ]
}
